<?xml version="1.0" encoding="UTF-8"?>
<!--Format:
name   => name of the animation sequence
start  => starting frame
end    => ending frame
loop   => looping animation (default)
-->
<animation name="survivor1_anim">
    <sequence name="idle"  start="0" end="19" loop="true" />
    <sequence name="move"  start="20" end="39" loop="true" />
    <sequence name="reload"  start="40" end="54" loop="false" />
    <sequence name="shoot"  start="55" end="57" loop="false" />
</animation>